{
  "$schema": "vscode://schemas/color-theme",
  "name": "Jellybeans Low Contrast",
  "type": "dark",
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "type": "#cf6a4c",
    "class": "#cf6a4c",
    "interface": "#cf6a4c",
    "enum": "#cf6a4c",
    "struct": "#cf6a4c",
    "typeParameter": "#cf6a4c",
    "namespace": "#cf6a4c"
  },
  "colors": {
    "editor.background": "#151515",
    "editor.foreground": "#e8e8d3",
    "editor.lineHighlightBackground": "#1f1f1fbf",
    "editor.selectionBackground": "#3f3f3faa",
    "editorCursor.foreground": "#8fbfdc",
    "editorWhitespace.foreground": "#4b4e53",
    "editorLineNumber.activeForeground": "#fabb6e",

    // Workbench backgrounds â€“ very low contrast vs editor
    "sideBar.background": "#151515",
    "sideBar.foreground": "#e8e8d3",
    "activityBar.background": "#151515",
    "activityBar.foreground": "#e8e8d3",
    "activityBar.inactiveForeground": "#4b4e53",
    "activityBarBadge.background": "#8197BF",
    "activityBarBadge.foreground": "#151515",
    "panel.background": "#151515",
    "panelTitle.activeForeground": "#e8e8d3",
    "panelTitle.inactiveForeground": "#888888",
    "statusBar.background": "#151515",
    "statusBar.debuggingBackground": "#151515",
    "statusBar.noFolderBackground": "#151515",
    "statusBar.foreground": "#e8e8d3",
    "titleBar.activeBackground": "#151515",
    "titleBar.inactiveBackground": "#151515",
    "titleBar.activeForeground": "#e8e8d3",
    "titleBar.inactiveForeground": "#888888",
    "titleBar.border": "#2a2a2a",
    "quickInputList.focusBackground": "#1c1c1c",

    // Editor groups & tabs
    "editorGroup.border": "#2a2a2a",
    "editorGroupHeader.tabsBackground": "#151515",
    "editorGroupHeader.noTabsBackground": "#151515",
    "tab.activeBackground": "#151515",
    "tab.inactiveBackground": "#151515",
    "tab.activeForeground": "#e8e8d3",
    "tab.inactiveForeground": "#888888",
    "tab.border": "#2a2a2a",
    "tab.activeBorderTop": "#fad07a",
    "tab.activeBorder": "#00000000",
    "tab.unfocusedActiveBorder": "#00000000",

    // Lists
    "list.activeSelectionBackground": "#262626",
    "list.activeSelectionForeground": "#ffffff",
    "list.dropBackground": "#062f4a",
    "list.errorForeground": "#f88070",
    "list.focusBackground": "#1c1c1c",
    "list.highlightForeground": "#8197BF",
    "list.hoverBackground": "#202020",
    "list.inactiveSelectionBackground": "#222222",
    "list.invalidItemForeground": "#b89500",
    "list.warningForeground": "#4d9e4d",
    "list.deemphasizedForeground": "#a0a093",

    // Inputs
    "input.background": "#1d1d1d",
    "input.foreground": "#cccccc",
    "input.placeholderForeground": "#cccccc80",
    "inputOption.activeBorder": "#8197BF",
    "inputValidation.errorBackground": "#5a1d1d",
    "inputValidation.errorBorder": "#be1100",
    "inputValidation.infoBackground": "#1d1d1d",
    "inputValidation.infoBorder": "#007acc",
    "inputValidation.warningBackground": "#352a05",
    "inputValidation.warningBorder": "#b89500",

    // Suggest widget / quick pick
    "editorSuggestWidget.background": "#1d1d1d",
    "editorSuggestWidget.border": "#2a2a2a",
    "editorSuggestWidget.foreground": "#e8e8d3",
    "editorSuggestWidget.highlightForeground": "#8197BF",
    "editorSuggestWidget.selectedBackground": "#262626",

    // Peek view
    "peekViewEditor.background": "#1d1d1d",
    "peekViewEditor.matchHighlightBackground": "#3399ff33",
    "peekViewEditorGutter.background": "#1d1d1d",
    "peekViewResult.selectionBackground": "#3399ff33",
    "peekViewResult.selectionForeground": "#ffffff",

    // Terminal
    "terminal.ansiWhite": "#e8e8d3",
    "terminal.ansiBrightWhite": "#e8e8d3",
    "terminal.ansiBlack": "#e8e8d3",
    "terminal.ansiBrightBlack": "#e8e8d3",
    "terminal.foreground": "#e4e4db",
    "terminal.background": "#151515",
    "terminal.ansiRed": "#cf6a4c",
    "terminal.ansiBrightRed": "#cf6a4c",
    "terminal.ansiYellow": "#fad07a",
    "terminal.ansiBrightYellow": "#fad07a",
    "terminal.ansiGreen": "#99ad6a",
    "terminal.ansiBrightGreen": "#99ad6a",
    "terminal.ansiBlue": "#8197BF",
    "terminal.ansiBrightBlue": "#8197BF",
    "terminal.ansiCyan": "#6796e6",
    "terminal.ansiBrightCyan": "#6796e6",
    "terminal.ansiBrightMagenta": "#c7a4ff",
    "terminal.ansiMagenta": "#c7a4ff",

    // Icons & problems
    "symbolIcon.methodForeground": "#fad07a",
    "symbolIcon.functionForeground": "#fad07a",
    "symbolIcon.constructorForeground": "#fad07a",
    "symbolIcon.variableForeground": "#8197BF",
    "symbolIcon.fieldForeground": "#8197BF",
    "symbolIcon.referenceForeground": "#8197BF",
    "symbolIcon.enumeratorForeground": "#8197BF",
    "symbolIcon.enumeratorMemberForeground": "#8197BF",
    "symbolIcon.classForeground": "#fdab47",
    "symbolIcon.interfaceForeground": "#fdab47",
    "symbolIcon.moduleForeground": "#fdab47",
    "symbolIcon.namespaceForeground": "#fdab47",
    "symbolIcon.keywordForeground": "#e8e8d3",
    "symbolIcon.unitForeground": "#e8e8d3",
    "symbolIcon.snippetForeground": "#e8e8d3",
    "problemsErrorIcon.foreground": "#cf6a4c",
    "problemsWarningIcon.foreground": "#fad07a",
    "problemsInfoIcon.foreground": "#8197BF",
    "notificationsErrorIcon.foreground": "#cf6a4c",
    "notificationsInfoIcon.foreground": "#fad07a",
    "notificationsWarningIcon.foreground": "#8197BF",
    "editorLightBulb.foreground": "#fad07a",
    "editorLightBulbAutoFix.foreground": "#fad07a",

    // Git decorations
    "gitDecoration.addedResourceForeground": "#99ad6a",
    "gitDecoration.modifiedResourceForeground": "#fad07a",
    "gitDecoration.deletedResourceForeground": "#cf6a4c",
    "gitDecoration.untrackedResourceForeground": "#8197BF",
    "gitDecoration.ignoredResourceForeground": "#777777",
    "gitDecoration.conflictingResourceForeground": "#c7a4ff",
    "gitDecoration.submoduleResourceForeground": "#8197BF",
    "gitDecoration.renamedResourceForeground": "#8197BF",

    // Diff editor & git gutter
    "diffEditor.insertedTextBackground": "#99ad6a33",
    "diffEditor.removedTextBackground": "#cf6a4c33",
    "diffEditor.insertedLineBackground": "#99ad6a22",
    "diffEditor.removedLineBackground": "#cf6a4c22",
    "editorGutter.addedBackground": "#99ad6a",
    "editorGutter.modifiedBackground": "#fad07a",
    "editorGutter.deletedBackground": "#cf6a4c",
    "editorGutter.commentRangeForeground": "#4b4e53",

    // Editor selection
    "selection.background": "#3f3f3f99",
    "editor.selectionHighlightBackground": "#3f3f3f55",

    // Focus / drag / outline colors
    "focusBorder": "#8197BF",
    "list.focusOutline": "#8197BF",
    "sash.hoverBorder": "#8197BF",

    // Bracket pair colors
    "editorBracketHighlight.foreground1": "#99ad6a",
    "editorBracketHighlight.foreground2": "#8197BF",
    "editorBracketHighlight.foreground3": "#fad07a",
    "editorBracketHighlight.foreground4": "#cf6a4c",
    "editorBracketHighlight.foreground5": "#c7a4ff",
    "editorBracketHighlight.foreground6": "#6796e6",
    "editorBracketHighlight.unexpectedBracket.foreground": "#cf6a4c",
    "editorBracketMatch.border": "#6796e6",
    "editorBracketMatch.background": "#00000000",
    "editorBracketPairGuide.activeBackground1": "#00000000",
    "editorBracketPairGuide.activeBackground2": "#00000000",
    "editorBracketPairGuide.activeBackground3": "#00000000",
    "editorBracketPairGuide.activeBackground4": "#00000000",
    "editorBracketPairGuide.activeBackground5": "#00000000",
    "editorBracketPairGuide.activeBackground6": "#00000000",
    "editorBracketPairGuide.background1": "#00000000",
    "editorBracketPairGuide.background2": "#00000000",
    "editorBracketPairGuide.background3": "#00000000",
    "editorBracketPairGuide.background4": "#00000000",
    "editorBracketPairGuide.background5": "#00000000",
    "editorBracketPairGuide.background6": "#00000000",

    // Borders instead of shadows
    "sideBar.border": "#2a2a2a",
    "sideBarActivityBarTop.border": "#2a2a2a",
    "panel.border": "#2a2a2a",
    "terminal.border": "#2a2a2a",
    "statusBar.border": "#2a2a2a",
    "activityBar.border": "#2a2a2a",
    "editorGroupHeader.tabsBorder": "#2a2a2a",
    "tab.lastPinnedBorder": "#2a2a2a",
    "widget.shadow": "#00000000",
    "scrollbar.shadow": "#00000000"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#888888"
      }
    },
    {
      "name": "Comments: Preprocessor",
      "scope": "comment.block.preprocessor",
      "settings": {
        "foreground": "#888888"
      }
    },
    {
      "name": "Comments: Documentation",
      "scope": ["comment.documentation", "comment.block.documentation"],
      "settings": {
        "foreground": "#858585"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator",
        "punctuation.separator.pointer-access.c",
        "punctuation.separator.pointer-access.cpp"
      ],
      "settings": {
        "foreground": "#ffe2a9"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage",
        "keyword.operator.logical.python",
        "keyword.operator.expression.in",
        "keyword.operator.expression.of",
        "keyword.operator.expression.typeof",
        "keyword.operator.expression.instanceof",
        "keyword.operator.new"
      ],
      "settings": {
        "foreground": "#8197BF"
      }
    },
    {
      "name": "Types",
      "scope": ["storage.type", "support.type"],
      "settings": {
        "foreground": "#8197BF"
      }
    },
    {
      "name": "Language Constants",
      "scope": [
        "constant.language",
        "variable.parameter.function.language.special.self.python",
        "support.constant",
        "variable.language"
      ],
      "settings": {
        "foreground": "#8197BF"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "support.variable"],
      "settings": {
        "foreground": "#ddddc1"
      }
    },
    {
      "name": "Functions (declarations and calls)",
      "scope": [
        "entity.name.function",
        "support.function",
        "variable.function",
        "meta.function-call",
        "meta.method-call",
        "meta.function-call entity.name.function",
        "meta.function-call support.function",
        "meta.method-call entity.name.function",
        "meta.method-call support.function",
        "meta.function-call variable.function",
        "meta.method-call variable.function"
      ],
      "settings": {
        "foreground": "#fad07a"
      }
    },
    {
      "name": "Object and type properties / fields (non-function identifiers)",
      "scope": [
        "meta.object-literal.key",
        "meta.property.object",
        "meta.property.class",
        "meta.property.declaration",
        "variable.other.property",
        "variable.other.member",
        "support.variable.property",
        "support.variable.property.dom",
        "entity.name.variable.property",
        "meta.type.property.ts",
        "meta.interface.body.ts entity.name.type"
      ],
      "settings": {
        "foreground": "#ddddc1"
      }
    },
    {
      "name": "Function parameters python",
      "scope": [
        "variable.parameter.function.language.python",
        "meta.function-call.arguments.python",
        "variable.parameter.function-call.python"
      ],
      "settings": {
        "foreground": "#ddddc1"
      }
    },
    {
      "name": "Directive punctuation c",
      "scope": ["punctuation.definition.directive.c", "punctuation.definition.directive.cpp"],
      "settings": {
        "foreground": "#8197BF"
      }
    },
    {
      "name": "Classes",
      "scope": [
        "meta.return-type",
        "support.class",
        "entity.other.inherited-class",
        "support.type",
        "entity.name.class",
        "storage.type.numeric.go",
        "storage.type.byte.go",
        "storage.type.boolean.go",
        "storage.type.string.go",
        "storage.type.uintptr.go",
        "storage.type.cs",
        "storage.type.generic.cs",
        "storage.type.modifier.cs",
        "storage.type.variable.cs",
        "storage.type.annotation.java",
        "storage.type.generic.java",
        "storage.type.java",
        "storage.type.object.array.java",
        "storage.type.primitive.array.java",
        "storage.type.primitive.java",
        "storage.type.token.java",
        "storage.type.groovy",
        "storage.type.annotation.groovy",
        "storage.type.parameters.groovy",
        "storage.type.generic.groovy",
        "storage.type.object.array.groovy",
        "storage.type.primitive.array.groovy",
        "storage.type.primitive.groovy"
      ],
      "settings": {
        "foreground": "#cf6a4c"
      }
    },
    {
      "name": "Type alias names (after `type`)",
      "scope": ["entity.name.type", "entity.name.type.ts", "entity.name.type.alias", "entity.name.type.alias.ts"],
      "settings": {
        "foreground": "#cf6a4c"
      }
    },
    {
      "name": "Numbers, Characters",
      "scope": ["constant.numeric", "constant.character"],
      "settings": {
        "foreground": "#cf6a4c"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "punctuation.definition.string.begin",
        "punctuation.definition.string.end",
        "punctuation.definition.string.template.begin",
        "punctuation.definition.string.template.end",
        "punctuation.definition.template-expression.begin",
        "punctuation.definition.template-expression.end",
        "constant.character.format.placeholder.other.python"
      ],
      "settings": {
        "foreground": "#99ad6a"
      }
    },
    {
      "name": "Symbol",
      "scope": "constant.other",
      "settings": {
        "foreground": "#8197BF"
      }
    },
    {
      "name": "Regexp",
      "scope": "string.regexp",
      "settings": {
        "foreground": "#cf6a4c"
      }
    },
    {
      "name": "Punctuation",
      "scope": "punctuation",
      "settings": {
        "foreground": "#e8e8d3"
      }
    },
    {
      "name": "JSON key",
      "scope": "support.type.property-name.json.comments",
      "settings": {
        "foreground": "#fbe3bf"
      }
    },
    {
      "name": "HTML: Doctype Declaration",
      "scope": [
        "meta.tag.sgml.doctype",
        "meta.tag.sgml.doctype string",
        "meta.tag.sgml.doctype entity.name.tag",
        "meta.tag.sgml punctuation.definition.tag.html"
      ],
      "settings": {
        "foreground": "#fabb6e"
      }
    },
    {
      "name": "HTML: Tags",
      "scope": [
        "meta.tag",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin",
        "punctuation.definition.tag.end"
      ],
      "settings": {
        "foreground": "#888888"
      }
    },
    {
      "name": "HTML: Tag Names (native elements)",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#fabb6e"
      }
    },
    {
      "name": "React / JSX Components",
      "scope": ["support.class.component.js", "support.class.component.tsx", "entity.name.type.module.jsx"],
      "settings": {
        "foreground": "#fabb6e"
      }
    },
    {
      "name": "HTML: Attribute Names",
      "scope": ["meta.tag entity.other.attribute-name", "entity.other.attribute-name.html"],
      "settings": {
        "foreground": "#8197BF"
      }
    },
    {
      "name": "HTML: Entities",
      "scope": ["constant.character.entity", "punctuation.definition.entity"],
      "settings": {
        "foreground": "#cf6a4c"
      }
    },
    {
      "name": "JSX: Foreground",
      "scope": "meta.jsx.children.js.jsx",
      "settings": {
        "foreground": "#ddddc1"
      }
    },
    {
      "name": "CSS: Selectors",
      "scope": [
        "meta.selector",
        "meta.selector entity",
        "meta.selector entity punctuation",
        "punctuation.definition.entity.css",
        "entity.name.tag.css"
      ],
      "settings": {
        "foreground": "#fabb6e"
      }
    },
    {
      "name": "CSS: Property Names",
      "scope": ["meta.property-name", "support.type.property-name"],
      "settings": {
        "foreground": "#ddddc1"
      }
    },
    {
      "name": "CSS: Property Values",
      "scope": [
        "meta.property-value",
        "meta.property-value constant.other",
        "support.constant.property-value",
        "punctuation.definition.constant.css"
      ],
      "settings": {
        "foreground": "#cf6a4c"
      }
    },
    {
      "name": "CSS: Property Units",
      "scope": [
        "keyword.other.unit.rem.css",
        "keyword.other.unit.vh.css",
        "keyword.other.unit.px.css",
        "keyword.other.unit.em.css",
        "keyword.other.unit.deg.css",
        "keyword.other.unit.percentage.css"
      ],
      "settings": {
        "foreground": "#cf6a4c"
      }
    },
    {
      "name": "CSS: Important Keyword",
      "scope": "keyword.other.important",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "CSS: Property value",
      "scope": "support.constant.property-value.css",
      "settings": {
        "foreground": "#8197BF"
      }
    },
    {
      "name": "JSDoc: definition block tag",
      "scope": ["punctuation.definition.block.tag.jsdoc", "punctuation.definition.inline.tag.jsdoc"],
      "settings": {
        "foreground": "#8197BF"
      }
    },
    {
      "name": "JSDoc: punctuation curly brackets",
      "scope": ["punctuation.definition.bracket.curly.begin.jsdoc", "punctuation.definition.bracket.curly.end.jsdoc"],
      "settings": {
        "foreground": "#fabb6e"
      }
    },
    {
      "name": "Clojure: Keyword",
      "scope": ["constant.keyword.clojure"],
      "settings": {
        "foreground": "#ffe2a9"
      }
    },
    {
      "name": "Markdown: Heading ",
      "scope": ["markup.heading.markdown", "punctuation.definition.heading.markdown"],
      "settings": {
        "foreground": "#fabb6e"
      }
    },
    {
      "name": "Markdown: Link",
      "scope": [
        "meta.link.inline.markdown",
        "markup.underline.link.markdown",
        "punctuation.definition.metadata.markdown",
        "markup.underline.link.image.markdown",
        "markup.underline.link.markdown",
        "punctuation.definition.link.markdown",
        "meta.link.email.lt-gt.markdown"
      ],
      "settings": {
        "foreground": "#99ad6a"
      }
    },
    {
      "name": "Markdown: Punctuation, Image",
      "scope": [
        "punctuation.definition.list.begin.markdown",
        "punctuation.definition.link.description.begin.markdown",
        "punctuation.definition.link.description.end.markdown",
        "string.other.link.description.markdown",
        "string.other.link.title.markdown",
        "punctuation.definition.link.title.begin.markdown",
        "punctuation.definition.link.title.end.markdown",
        "punctuation.definition.constant.begin.markdown",
        "punctuation.definition.constant.end.markdown",
        "punctuation.definition.constant.markdown"
      ],
      "settings": {
        "foreground": "#8197BF"
      }
    },
    {
      "name": "Markdown Quote",
      "scope": ["punctuation.definition.quote.begin.markdown"],
      "settings": {
        "foreground": "#cf6a4c"
      }
    },
    {
      "name": "Markdown: Code Block and others",
      "scope": [
        "punctuation.definition.markdown",
        "punctuation.definition.raw.markdown",
        "markup.fenced_code.block.markdown",
        "markup.raw.block.markdown",
        "markup.inline.raw.string.markdown"
      ],
      "settings": {
        "foreground": "#888888"
      }
    },
    {
      "name": "Markdown: Strong",
      "scope": ["markup.bold.markdown", "punctuation.definition.bold.markdown"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown: Emphasize",
      "scope": ["markup.italic.markdown", "punctuation.definition.italic.markdown"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Latex arguments begin and end",
      "scope": [
        "punctuation.definition.arguments.begin.math.tex",
        "punctuation.definition.arguments.end.math.tex",
        "punctuation.math.end.bracket.curly",
        "punctuation.math.begin.bracket.curly"
      ],
      "settings": {
        "foreground": "#8197BF"
      }
    },
    {
      "name": "Latex Punctuation",
      "scope": [
        "punctuation.definition.function.tex",
        "punctuation.definition.function.math.tex",
        "punctuation.definition.function.latex",
        "keyword.control.preamble.latex"
      ],
      "settings": {
        "foreground": "#fad07a"
      }
    },
    {
      "name": "Latex & Markdown Math",
      "scope": ["punctuation.definition.math.begin.markdown", "punctuation.definition.math.end.markdown"],
      "settings": {
        "foreground": "#99ad6a"
      }
    },
    {
      "name": "Latex Constant",
      "scope": ["punctuation.definition.constant.math.tex"],
      "settings": {
        "foreground": "#cf6a4c"
      }
    }
  ]
}
